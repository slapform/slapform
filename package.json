{
  "name": "slapform",
  "version": "2.0.11",
  "description": "Easily send submission to Slapform, your favorite form backend service.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slapform/slapform"
  },
  "keywords": [
    "html",
    "slapform",
    "html-form",
    "html-forms",
    "html forms",
    "form",
    "forms",
    "form-builder",
    "form builder",
    "form-builder-api",
    "form builder api",
    "form-generator",
    "form generator",
    "formbuilder",
    "form backend"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slapform/slapform/issues"
  },
  "homepage": "https://slapform.com",
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "itwcw-package-analytics": "^1.0.4"
  },
  "devDependencies": {
    "fs-jetpack": "^4.1.0",
    "mocha": "^8.4.0",
    "prepare-package": "^1.1.12"
  }
}